{
    "id": "nl.emphisia.icon",
    "runtime": "org.gnome.Platform",
    "runtime-version": "49",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "Iconic",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "RUST_LOG": "Iconic=info"
        }
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name" : "Iconic",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type": "archive",
                    "url" : "https://github.com/youpie/Iconic/releases/download/v2025.9.1/Iconic-2025.9.1.tar.xz",
                    "sha256" : "5b5732aeb6dd8482b64bef95e8a85c239b23ef14bbce1ec7e04fa64787dd735f"
                    
                }
            ]
        }
    ]
}
